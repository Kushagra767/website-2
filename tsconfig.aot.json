{
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2015",
			"dom"
		],
		"outDir": "aot",
		"noImplicitAny": false,
		"suppressImplicitAnyIndexErrors": true
	},
	"files": [
		"src/app/app.module.ts",
		"src/main.ts"
	],
	"angularCompilerOptions": {
		"genDir": "aot",
		"skipMetadataEmit": true
	}
}

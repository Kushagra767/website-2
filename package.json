{
  "name": "rheinklang-kollektiv",
  "version": "0.0.1-beta.39",
  "scripts": {
    "ng": "ng",
    "prestart": "run-s create:*",
    "start": "ng serve",
    "create:dev-env": "node bin/create-env",
    "create:version": "node bin/create-version",
    "prebuild": "run-s create:*",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "preproduction": "npm run create:version",
    "production": "ng build --configuration=production --aot=true --buildOptimizer=true --optimization=true",
    "postproduction": "node bin/copy-apache-config",
    "prettier": "prettier --write './src/**/*.{css,scss,tsx,ts,js,json}'",
    "deploy:development": "node -r dotenv/config deploy.js --tag=development",
    "deploy:production": "node -r dotenv/config deploy.js --tag=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "8.2.2",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@glidejs/glide": "3.3.0",
    "@ngrx/effects": "8.2.0",
    "@ngrx/router-store": "8.2.0",
    "@ngrx/store": "8.2.0",
    "@ngrx/store-devtools": "8.2.0",
    "apollo-angular": "1.6.0",
    "apollo-angular-link-http": "^1.6.0",
    "apollo-cache-inmemory": "^1.3.2",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.0",
    "lazysizes": "5.1.1",
    "microtip": "0.2.2",
    "normalize.css": "8.0.1",
    "rxjs": "~6.4.0",
    "svgxuse": "1.2.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.0",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/chalk": "2.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/ora": "3.2.0",
    "chalk": "2.4.2",
    "codelyzer": "^5.0.0",
    "dotenv": "8.1.0",
    "ftp-deploy": "2.3.3",
    "git-cz": "3.2.1",
    "git-describe": "4.0.4",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "4.1.5",
    "ora": "3.4.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
